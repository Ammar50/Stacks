    <header>
        <a href="http://ravendb.net/"><img src="http://static.ravendb.net/badges/standard/badge1.png" alt="Powered by RAVENDB" /></a>
    </header>

    <nav class="clearfix">
        @Html.RawActionLink("<img src=\"/Content/Images/Home.png\" alt=\"Home\" /><span>Home</span>", "Index", "RavenDB", null, new { @class = "ui-linkbutton home" })
        @Html.RawActionLink("<img src=\"/Content/Images/processing.png\" alt=\"Settings\" /><span>Settings</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton home" })
        @Html.RawActionLink("<span>Products</span>", "Product", "RavenDB", null, new { @class = "ui-linkbutton" })
        @Html.RawActionLink("<span>Categories</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton" })
        @Html.RawActionLink("<span>Suppliers</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton" })
        @Html.RawActionLink("<span>Discounts</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton" })
        @Html.RawActionLink("<span>Orders</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton" })
        @Html.RawActionLink("<span>Invoicing</span>", "Category", "RavenDB", null, new { @class = "ui-linkbutton" })        
    </nav>

    <div role="main">
        @RenderBody()
    </div>

    <footer>
    </footer>
      
